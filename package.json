{
    "name": "projetss6-tower",
    "version": "1.0.0",
    "description": "Tower defense game in TypeScript.",
    "scripts": {
        "build": "npx tsc -p tsconfig.json",
        "clean": "rm -f $(find . -name \\*~)",
        "eslint": "npx eslint src tst",
        "start": "node --experimental-specifier-resolution=node dist/motor.js",
        "main": "node --experimental-specifier-resolution=node dist/motor.js",
        "parcel": "parcel src/index.html",
        "bundle": "parcel build src/index.html",
        "test": "npx jest",
        "watch": "npx tsc -p tsconfig.json --watch"
    },
    "keywords": [],
    "author": "Mathias APARICIO, Hugo Bastien, Mohamed Rekik, Mathias ROSA",
    "license": "ISC",
    "type": "module",
    "dependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.3",
        "commander": "^10.0.0",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "eslint": "^8.36.0",
        "jest": "^29.5.0",
        "parcel": "^2.8.3",
        "path-browserify": "^1.0.1",
        "process": "^0.11.10",
        "typescript": "^4.9.5"
    }
